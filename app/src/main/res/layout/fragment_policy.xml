<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.synolexoil.synolexappandroidstudio151sdk422api17.Policy">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="کاربران گرامی"
        android:id="@+id/textView25"
        android:layout_marginTop="205dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="10dp" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:src="#FFCE0101"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:src="@drawable/logohorizontal"
        android:layout_alignTop="@+id/imageView26"
        android:layout_alignParentStart="true"
        android:layout_margin="20dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="تعهدات سینولکس"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#ffffff"
        android:textSize="17dp"
        android:textStyle="bold"
        android:layout_alignBottom="@+id/imageView26"
        android:layout_alignEnd="@+id/textView25"
        android:layout_marginBottom="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="اطلاعات شخصی شما نزد شرکت سینولکس محفوظ می باشد و صرفا جهت رفاه حال شما عزیزان در استفاده بهتر از امکانات این اپلیکیشن اخذ می گردد."
        android:id="@+id/textView27"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/textView25"
        android:layout_alignEnd="@+id/textView25"
        android:layout_alignStart="@+id/imageView27" />

</RelativeLayout>
